<div class="section">
  <div class="container">
    <div class="row">
      <form class="col s12" action="/emails/generate" method="GET">
        <div class="row">
          <div class="input-field col l6 s12">
            <h5 class="center">List Size</h5>
            <select id="list_size" name="list_size">
              <option>10</option>
              <option>100</option>
              <option selected="selected">1000</option>
              <option>10000</option>
              <option>100000</option>
            </select>
          </div>
          <div class="input-field col l6 s12">
            <h5 class="center">Duplication Rate</h5>
            <select id="duplication_rate" name="duplication_rate">
              <option value="0">  0 %</option>
              <option value="0.25"> 25 %</option>
              <option value="0.50" selected="selected"> 50 %</option>
              <option value="0.75"> 75 %</option>
              <option value="1">100 %</option>
            </select>
          </div>

          <div class="col s12 center">
            <br/><br/>
            <div class="long-job-button-row">
              <button class="btn-large waves-effect waves-light" type="submit">Generate
                <i class="material-icons right">play_circle_filled</i>
              </button>
              <div class="progress show-progress" style="display: none">
                <div class="indeterminate"></div>
              </div>
            </div>
          </div>
        </div>
      </form>
    </div>
  </div>
</div>